{
  "name": "@providerprotocol/agents",
  "version": "0.0.6",
  "description": "Unified Agent Protocol (UAP) 1.0 implementation for @providerprotocol/ai",
  "type": "module",
  "author": {
    "name": "Provider Protocol"
  },
  "license": "MIT",
  "keywords": [
    "ai",
    "agent",
    "agents",
    "llm",
    "unified-agent-protocol",
    "uap",
    "anthropic",
    "openai",
    "google",
    "gemini",
    "ollama",
    "multi-provider",
    "tool-calling",
    "agentic",
    "middleware",
    "thread",
    "checkpoint",
    "ai-sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/providerprotocol/agents"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./execution": {
      "types": "./dist/execution/index.d.ts",
      "import": "./dist/execution/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    },
    "./thread-tree": {
      "types": "./dist/thread-tree/index.d.ts",
      "import": "./dist/thread-tree/index.js"
    },
    "./checkpoint": {
      "types": "./dist/checkpoint/index.d.ts",
      "import": "./dist/checkpoint/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "bun run build",
    "test": "bun test",
    "test:unit": "bun test tests/unit",
    "test:live": "bun test tests/live",
    "lint": "eslint src tests examples --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "docgen": "bun run scripts/docgen.ts",
    "docs:sort": "bun run scripts/sort-docs.ts",
    "docs": "bun run docgen && bun run docs:sort",
    "docs:deploy": "bun run docs && rm -rf ../UPP-1/docs/src/content/docs/agents && cp -r docs-sorted ../UPP-1/docs/src/content/docs/agents && rm -rf ../UPP-1/docs/.astro ../UPP-1/docs/node_modules/.astro"
  },
  "peerDependencies": {
    "@providerprotocol/ai": ">=0.0.11"
  },
  "devDependencies": {
    "@providerprotocol/ai": "^0.0.12",
    "@types/bun": "^1.3.5",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3"
  }
}
